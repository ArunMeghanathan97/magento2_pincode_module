<?xml version="1.0"?>
 
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../lib/internal/Magento/Framework/Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Mbf_Pincodenurams::pincodenurams" title="Simple Pincode"
                    sortOrder="100">
                    <resource id="Mbf_Pincodenurams::add_pincodenurams" title="Add Pincode"
